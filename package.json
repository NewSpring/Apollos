{
  "name": "Apollos",
  "version": "1.0.0",
  "repository": "https://github.com/NewSpring/Apollos.git",
  "author": "differential",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.12",
    "@storybook/addon-links": "^3.2.12",
    "@storybook/react": "^3.2.12",
    "@storybook/react-native": "^3.2.12",
    "babel-eslint": "^8.0.1",
    "dotenv": "^4.0.0",
    "eslint": "4.8.0",
    "eslint-config-airbnb": "16.0.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jest": "21.2.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "jest-expo": "^21.0.2",
    "react-native-scripts": "1.5.0",
    "react-native-storybook-loader": "^1.5.1",
    "react-scripts": "^1.0.14",
    "react-test-renderer": "16.0.0-alpha.12"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "npm run create-app-entrypoint && npm run run-with-settings 'react-native-scripts start'",
    "eject": "react-native-scripts eject",
    "android": "npm run create-app-entrypoint && npm run run-with-settings 'react-native-scripts android'",
    "ios": "npm run create-app-entrypoint && npm run run-with-settings 'react-native-scripts ios'",
    "web": "npm run create-app-entrypoint && npm run run-with-settings 'react-scripts start'",
    "build": "npm run create-app-entrypoint && npm run run-with-settings 'react-scripts build'",
    "test": "npm run lint && $(npm bin)/jest",
    "test-watcher": "node node_modules/jest/bin/jest.js --watch",
    "lint": "$(npm bin)/eslint .",
    "storybook-native-start": "npm run create-storybook-entrypoint && npm run storybook-load-stories && storybook start -p 7007 -c src/.storybookNative",
    "storybook-web-start": "npm run create-storybook-entrypoint && npm run storybook-load-stories && start-storybook -p 9009 -s public -c src/.storybookWeb",
    "storybook-ios": "npm run create-storybook-entrypoint && react-native-scripts ios",
    "storybook-android": "npm run create-storybook-entrypoint && react-native-scripts android",
    "storybook-load-stories": "rnstl",
    "build-storybook": "build-storybook -s public",
    "create-app-entrypoint": "node ./.scripts/create-app-entrypoint.js",
    "create-storybook-entrypoint": "node ./.scripts/create-storybook-entrypoint.js",
    "run-with-settings": "node ./.scripts/run-with-settings.js"
  },
  "jest": {
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)(spec|test|tests).js?(x)"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": "./src",
      "outputFile": "./src/.storybook.js",
      "pattern": "**/*.stories.js"
    }
  },
  "dependencies": {
    "expo": "^21.0.0",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "16.0.0-alpha.12",
    "react-dom": "^16.0.0",
    "react-native": "^0.48.4",
    "react-native-web": "^0.1.4",
    "react-router": "^4.2.0",
    "recompose": "^0.26.0"
  }
}
